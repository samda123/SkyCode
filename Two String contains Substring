using namespace std;

// Complete the twoStrings function below.
string twoStrings(string s1, string s2) 
{
    string r;int f=0;
    for(int i=0;i<s2.size();i++)
    {
        if(s1.find(s2[i])!=-1)
        {
            f=1;
            break;
        }
    }
if(f==1)
return "yes";
else 
return "no";
}
